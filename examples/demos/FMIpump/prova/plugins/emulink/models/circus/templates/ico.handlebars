<?xml version="1.0" encoding="UTF-8"?>
<obcs xmlns="http://www.irit.fr/ICS/OBCS/7.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" copyright="Equipe ICS - IRIT Universite Toulouse Paul Sabatier http://www.irit.fr/recherches/ICS/" name="{{filename}}" version="7" package="" xsi:schemaLocation="http://www.irit.fr/ICS/OBCS/7.0 https://www.irit.fr/recherches/ICS/xsd/petshop/v7/v7.xsd">
    <operationclusters>
        <operationcluster title="Default" position="1"/>
    </operationclusters>
    <layers>
        <layer name="CREATION" custom="false">
            <properties>
                <property name="visibility" value="true"/>
                <property name="description" value="Creation Layer"/>
            </properties>
        </layer>
        <layer name="DEFAULT" custom="false">
            <properties>
                <property name="visibility" value="true"/>
                <property name="description" value="Default Layer"/>
            </properties>
        </layer>
        <layer name="PARAM_SERVICES" custom="false">
            <properties>
                <property name="visibility" value="true"/>
                <property name="description" value="Param's services Layer"/>
            </properties>
        </layer>
        <layer name="PARAM_TEST_ARC" custom="false">
            <properties>
                <property name="visibility" value="true"/>
                <property name="description" value="Param's TestArcs Layer"/>
            </properties>
        </layer>
        <layer name="STATIC_PLACE" custom="false">
            <properties>
                <property name="visibility" value="true"/>
                <property name="description" value="NO ACTION NAME"/>
            </properties>
        </layer>
    </layers>
    <places>{{#each states}}
        <place kind="SIMPLE" name="{{name}}">
            <graphics>
                <graphic layer="DEFAULT" show_marking="true" show_name="true">
                    <center center_x="{{x}}" center_y="{{y}}"/>
                </graphic>
            </graphics>{{#if initial_state}}
            <token multiplicity="1"/>{{/if}}
        </place>{{/each}}
    </places>
    <transitions>{{#each transitions}}
        <transition kind="SIMPLE" name="{{id}}" synchronized="true">
            <graphics>
                <graphic allintitle="true" eventvisible="false" layer="DEFAULT" show_action="false" show_name="true" show_precondition="false" show_time="false">
                    <center center_x="{{controlPoint.x}}" center_y="{{controlPoint.y}}"/>
                </graphic>
            </graphics>
            <action></action>
            <precondition>true</precondition>
            <time></time>
            <eventextension eventname="{{name}}" eventsourcename="">
                <preconditionextension>true</preconditionextension>
                <eventparameters/>
            </eventextension>
        </transition>{{/each}}
    </transitions>
    <arcs>{{#each transitions}}
        <arc kind="INPUT" place="{{source.name}}" transition="{{id}}" weight="1">
            <graphics>
                <graphic shape="LINE">
                    <label_origin x="0" y="0"/>
                </graphic>
            </graphics>
        </arc>
        <arc kind="OUTPUT" place="{{target.name}}" transition="{{id}}" weight="1">
            <graphics>
                <graphic shape="LINE">
                    <label_origin x="0" y="0"/>
                </graphic>
            </graphics>
        </arc>{{/each}}
    </arcs>
</obcs>
